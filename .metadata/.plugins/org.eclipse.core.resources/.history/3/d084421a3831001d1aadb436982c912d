#include "transformarTemperatura.h"

float transformarCelsiusAfaren(int tempCel)
{
	int tempFaren;

	tempFaren = (float)(tempCel * 9/5)+32;

	return tempFaren;
}

float transformarFarenAcelsius(int tempFaren)
{
	int tempCel;

	tempCel = (float)(tempFaren - 32)*9/5;

	return tempCel;
}
